{
  "AzureAd": {
    "Instance": "${AZURE_AD_INSTANCE}",
    "Domain": "${AZURE_AD_DOMAIN}",
    "TenantId": "${AZURE_AD_TENANT_ID}",
    "ClientId": "${AZURE_AD_CLIENT_ID}",
    "Scope": "${AZURE_AD_SCOPE}",
    "ValidIssuers": [
      "${AZURE_AD_VALID_ISSUER_1}",
      "${AZURE_AD_VALID_ISSUER_2}"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
